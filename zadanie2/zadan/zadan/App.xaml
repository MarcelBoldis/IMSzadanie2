<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="zadan.App">
    <Application.Resources>
        <ResourceDictionary>
            <OnPlatform x:TypeArguments="x:String" x:Key="NormalFont">
                <On Platform="Android" Value="UbuntuMedium.ttf#Ubuntu Medium" />
                <On Platform="UWP" Value="/Assets/Ubuntu-Medium.ttf#Open Sans" />
                <On Platform="iOS" Value="Ubuntu-Medium.ttf" />

            </OnPlatform>
        </ResourceDictionary>
    </Application.Resources>
</Application>